local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local LocalPlayer = Players.LocalPlayer

local validKeys = {
    "BRAINROT-A9X2L","BRAINROT-Z5Q8D","BRAINROT-K3L7V","BRAINROT-Y2T9M",
    "BRAINROT-N4X5P","BRAINROT-V7L1Q","BRAINROT-XQ3A8","BRAINROT-P9T6K",
    "BRAINROT-M2K5Z","BRAINROT-B8Y4N","BRAINROT-Q5P9X","BRAINROT-4L7KC",
    "BRAINROT-9ZD3M","BRAINROT-3Y2LV","BRAINROT-JX6QP","BRAINROT-LP5A7",
    "BRAINROT-1L2KD","BRAINROT-FX9N5","BRAINROT-A5K9B","BRAINROT-ZQ1NX"
}

local usedKeys = {}

local discordLink = "https://discord.gg/8uJDJaYNvw"
local keyLink = "https://rekonise.com/steal-a-brainrot-anti-hit-yv2ob"

local gui = Instance.new("ScreenGui", LocalPlayer:WaitForChild("PlayerGui"))
gui.Name = "BRAINUI"
gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

local blur = Instance.new("BlurEffect")
blur.Size = 10
blur.Name = "BrainBlur"
blur.Parent = Lighting

local function closeUI()
	if Lighting:FindFirstChild("BrainBlur") then
		Lighting.BrainBlur:Destroy()
	end
	gui:Destroy()
end

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 340, 0, 370)
frame.Position = UDim2.new(0.5, -170, 0.5, -185)
frame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
frame.BackgroundTransparency = 0.05
frame.Active = true
frame.Draggable = true
