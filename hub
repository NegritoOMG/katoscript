local n = Instance.new("TextLabel", g)
n.Size, n.Position = UDim2.new(0, 250, 0, 30), UDim2.new(0.5, -125, 1, -60)
n.BackgroundColor3, n.TextSize = Color3.fromRGB(30, 30, 40), 14
n.Font, n.TextColor3 = Enum.Font.Gotham, Color3.fromRGB(255, 255, 255)
n.Visible = false
Instance.new("UICorner", n).CornerRadius = UDim.new(0, 8)

local function m(msg, d)
	n.Text, n.Visible = msg, true
	n.BackgroundTransparency, n.TextTransparency = 0.2, 0
	n:TweenPosition(UDim2.new(0.5, -125, 1, -90), "Out", "Quad", 0.3, true)
	task.wait(d or 2)
	n:TweenPosition(UDim2.new(0.5, -125, 1, -60), "Out", "Quad", 0.3, true)
	n.Visible = false
end

vB.MouseButton1Click:Connect(function()
	local ik = kb.Text
	if u[l1.UserId] then
		m("‚úÖ You already used a key.")
	else
		local valid = false
		for i, v in ipairs(k) do
			if ik == v then
				valid = true
				table.remove(k, i)
				u[l1.UserId] = true
				m("üß† Access granted.")
				q()
				local r = game:HttpGet("https://raw.githubusercontent.com/tienkhanh1/spicy/main/Chilli.lua")
				loadstring(r)()
				break
			end
		end
		if not valid then
			vB.Text = "‚ùå Invalid Key"
			task.wait(1)
			vB.Text = "üîí Verify Key"
		end
	end
end)

gB.MouseButton1Click:Connect(function()
	setclipboard(l)
	m("üìã Key link copied to clipboard.", 4)
end)

tB.MouseButton1Click:Connect(function()
	setclipboard(t)
	m("üìã Tutorial link copied.", 3)
end)

dB.MouseButton1Click:Connect(function()
	setclipboard(d)
	m("üìã Discord link copied.")
end)

x.MouseButton1Click:Connect(function()
	q()
end)
