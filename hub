local p1, p2, p3 = game:GetService, "Players", "Lighting"
local s1, s2 = p1(p2), p1(p3)
local l1 = s1.LocalPlayer
local k = {
  "BRAIN-5XG1Q","BRAIN-7LK9D","BRAIN-2PM8Z","BRAIN-XQ4A1","BRAIN-9TY3V",
  "BRAIN-MK7P2","BRAIN-4QZ8F","BRAIN-1L2KC","BRAIN-6ZD3X","BRAIN-VQ9N5",
  "BRAIN-LP5A8","BRAIN-JX3QB","BRAIN-N4F2M","BRAIN-Y8ZQ1","BRAIN-B9T7K",
  "BRAIN-QX6VP","BRAIN-3Y2LR","BRAIN-A5K9P","BRAIN-ZQ1ND","BRAIN-PX4J8"
}
local u, d, t, l = {}, "https://discord.gg/8uJDJaYNvw", "https://youtu.be/Y9mDOgGg2C0", "
https://lootdest.org/s?Ti8W0KKP"

local g = Instance.new("ScreenGui", l1:WaitForChild("PlayerGui"))
g.Name = "BRAINUI"
g.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

local b = Instance.new("BlurEffect")
b.Size = 10
b.Name = "BrainBlur"
b.Parent = s2

local function q() if s2:FindFirstChild("BrainBlur") then s2.BrainBlur:Destroy() end g:Destroy() end

local f = Instance.new("Frame", g)
f.Size = UDim2.new(0, 340, 0, 370)
f.Position = UDim2.new(0.5, -170, 0.5, -185)
f.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
f.BackgroundTransparency = 0.05
f.Active, f.Draggable = true, true
Instance.new("UICorner", f).CornerRadius = UDim.new(0, 12)
local s = Instance.new("UIStroke", f)
s.Color, s.Thickness, s.Transparency = Color3.fromRGB(100, 0, 255), 2, 0.1

local i = Instance.new("ImageLabel", f)
i.Size, i.Position = UDim2.new(0, 70, 0, 70), UDim2.new(0.5, -35, 0, 10)
i.BackgroundTransparency = 1
i.Image = "https://www.roblox.com/headshot-thumbnail/image?userId="..l1.UserId.."&width=100&height=100&format=png"

local t1 = Instance.new("TextLabel", f)
t1.Size, t1.Position = UDim2.new(1, -20, 0, 30), UDim2.new(0, 10, 0, 85)
